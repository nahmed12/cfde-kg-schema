{
    "nodes": [
        {
            "node": "Member",
            "example": [
                "Chris Kinsinger",
                "George Papanicolaou"
            ],
            "display": [
                {
                    "label": "Member ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                },
                {
                    "label": "affiliation",
                    "text": "${affiliation}",
                    "type": "text"
                },
                {
                    "label": "role",
                    "text": "${role}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label",
                "role"
            ]
        },
        {
            "node": "Affiliation",
            "example": [
                "R03",
                "4DN"
            ],
            "display": [
                {
                    "label": "Affiliation ID",
                    "text": "${id}",
                    "type": "text"
                },
                {
                    "label": "label",
                    "text": "${label}",
                    "type": "text"
                }
            ],
            "relation": [],
            "search": [
                "id",
                "label"
            ]
        }
    ],
    "edges": [
        {
            "match": [
                "Member Affiliation"
            ],
            "selected": true,
            "display": [
                {
                    "label": "source",
                    "text": "${source}",
                    "type": "text"
                },
                {
                    "label": "relation",
                    "text": "${relation}",
                    "type": "text"
                },
                {
                    "label": "target",
                    "text": "${target}",
                    "type": "text"
                },
                {
                    "label": "source label",
                    "text": "${source_label}",
                    "type": "text"
                },
                {
                    "label": "target label",
                    "text": "${target_label}",
                    "type": "text"
                }
            ]
        }
    ],
    "header": {
        "title": "CFDE SNKG",
        "icon": {
            "src": "https://raw.githubusercontent.com/nahmed12/cfde-kg-schema/main/images/CFDE_WORKBENCH_LOGO.png",
            "favicon": "https://raw.githubusercontent.com/nahmed12/cfde-kg-schema/main/images/CFDE_WORKBENCH_LOGO.png",
            "alt": "CFDE logo",
            "key": "cfde_logo",
            "width": 100,
            "height": 60
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "Term & Gene Search",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg"
                }
            }
        ]
    },
    "footer": [
        {
            "type": "github",
            "code": "https://github.com/MaayanLab/enrichr-kg",
            "issues": "https://github.com/MaayanLab/enrichr-kg/issues"
        },
        {
